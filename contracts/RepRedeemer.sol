pragma solidity 0.5.13;
pragma experimental ABIEncoderV2;

import "@daostack/arc/contracts/schemes/Locking4Reputation.sol";

contract RepRedeemer {

    function redeemLocking4Reputation(
        Locking4Reputation lt4Reputation,
        address[] memory lt4rRedeems) public {
        for (uint i = 0; i < lt4rRedeems.length; i++) {
            lt4Reputation.redeem(lt4rRedeems[i]);
        }
    }
}
